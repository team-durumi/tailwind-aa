{{- $.Scratch.Add "index" slice -}}
{{- range site.RegularPages -}}
    {{- $.Scratch.Add "index" (dict "title" .Title "tags" .Params.tags "subjects" .Params.subjects "creators" .Params.creators "venues" .Params.venues "sources" .Params.sources "contents" .Plain "permalink" .Permalink) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}